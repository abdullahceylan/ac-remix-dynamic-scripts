  "bugs": "https://github.com/abdullahceylan/ac-remix-dynamic-scripts/issues",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/react": "^19.0.1",
    "tsup": "^8.0.1",
